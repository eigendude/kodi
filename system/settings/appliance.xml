<?xml version="1.0" encoding="utf-8" ?>
<settings version="1">
  <section id="player">
    <category id="videoplayer">
      <group id="2">
        <setting id="videoplayer.errorinaspect">
          <default>20</default>
        </setting>
      </group>
    </category>
  </section>
  <section id="interface">
    <category id="screensaver">
      <group id="1">
        <setting id="screensaver.mode">
          <default></default>
        </setting>
      </group>
    </category>
    <category id="regional">
      <group id="2">
        <setting id="locale.timezonecountry">
          <default>United States</default>
        </setting>
        <setting id="locale.timezone">
          <default>America/Los_Angeles</default>
        </setting>
      </group>
    </category>
    <category id="other">
      <group id="2">
        <setting id="lookandfeel.startupwindow">
          <default>12600</default> <!-- WINDOW_WEATHER -->
        </setting>
      </group>
    </category>
  </section>
  <section id="system">
    <category id="input">
      <group id="2">
        <setting id="input.enablemouse">
          <default>false</default>
        </setting>
      </group>
    </category>
  </section>
  <section id="services">
    <category id="weather">
      <group id="1">
        <setting id="weather.addon">
          <default>weather.weatherbit.io</default>
        </setting>
      </group>
    </category>
  </section>
</settings>
